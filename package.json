{
  "name": "floss",
  "version": "1.0.0",
  "description": "Unit-testing for those hard to reach places",
  "bin": "./bin/floss.js",
  "main": "./index.js",
  "author": "Matt Karl <matt.karl@jibo.com>",
  "scripts": {
    "lint": "eslint index.js electron bin test",
    "pretest": "npm run lint",
    "test": "node bin/floss.js --path=test/index.js",
    "start": "npm test -- --debug"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jiborobot/floss.git"
  },
  "bugs": {
    "url": "https://github.com/jiborobot/floss/issues"
  },
  "homepage": "https://github.com/jiborobot/floss",
  "engines": {
    "node": ">=4.0"
  },
  "license": "MIT",
  "keywords": [
    "mocha",
    "sinon",
    "chai",
    "mocha-electron",
    "electron-mocha",
    "electron",
    "test",
    "tests",
    "unit-tests",
    "unit-testing"
    "travis",
    "travis-ci"
  ],
  "dependencies": {
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "mocha": "^2.4.5",
    "object-assign": "^4.1.0",
    "resolve": "^1.1.7",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "peerDependencies": {
    "electron-prebuilt": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^3.5.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "no-console": 0,
      "no-debugger": 1
    },
    "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "globals": {
      "expect": true
    }
  }
}
